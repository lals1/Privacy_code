[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/lals1
[access "refs/for/refs/*"]
	read = group user/lals1
	push = group user/lals1
	label-Code-Review = -2..+2 group user/lals1
	label-Verified = -1..+1 group user/lals1
	rebase = group user/lals1
	removeReviewer = group user/lals1
	submit = group user/lals1
[access "refs/heads/*"]
	read = group user/lals1
	create = group user/lals1
	push = +force group user/lals1
	pushMerge = group user/lals1
[access "refs/tags/*"]
	push = group user/lals1
	push = +force group user/lals1
